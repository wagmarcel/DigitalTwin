@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ift:WC0
  rdf:type ift:wasteclass ;
.
ift:WC1
  rdf:type ift:wasteclass ;
  ift:higherHazardLevel ift:WC0 ;
.
ift:WC2
  rdf:type ift:wasteclass ;
  ift:higherHazardLevel ift:WC1 ;
.
ift:WC3
  rdf:type ift:wasteclass ;
  ift:higherHazardLevel ift:WC2 ;
.
ift:cutter
  rdf:type rdfs:Class ;
  rdfs:subClassOf ift:machine ;
.
ift:entity
  rdf:type rdfs:Class ;
.
ift:filter
  rdf:type rdfs:Class ;
  rdfs:subClassOf ift:machine ;
.
ift:filterCartridge
  rdf:type rdfs:Class ;
  rdfs:subClassOf ift:entity ;
.
ift:higherHazardLevel
  rdf:type rdf:Property ;
  rdf:type owl:TransitiveProperty ;
  rdfs:domain ift:wasteclass ;
  rdfs:range ift:wasteclass ;
.
ift:lasercutter
  rdf:type rdfs:Class ;
  rdfs:subClassOf ift:cutter ;
.
ift:machine
  rdf:type rdfs:Class ;
.
ift:machinestate
  rdf:type rdfs:Class ;
.
ift:plasmacutter
  rdf:type rdfs:Class ;
  rdfs:subClassOf ift:cutter ;
.
ift:severityClass
  rdf:type rdfs:Class ;
.
ift:severityCode
  rdf:type rdf:Property ;
  rdfs:domain ift:severityClass ;
  rdfs:range xsd:string ;
.
ift:severityCritical
  rdf:type ift:severityClass ;
  ift:severityCode "critical" ;
.
ift:severityWarning
  rdf:type ift:severityClass ;
  ift:severityCode "warning" ;
.
ift:stateValidFor
  rdf:type rdf:Property ;
  rdfs:domain ift:machinestate ;
  rdfs:range ift:machine ;
.
ift:state_CLEANING
  rdf:type ift:machinestate ;
  rdfs:label "CLEANING" ;
  ift:stateValidFor ift:filter ;
.
ift:state_CLEARING
  rdf:type ift:machinestate ;
  rdfs:label "CLEARING" ;
  ift:stateValidFor ift:cutter ;
.
ift:state_ERROR
  rdf:type ift:machinestate ;
  rdfs:label "ERROR" ;
  ift:stateValidFor ift:cutter ;
  ift:stateValidFor ift:filter ;
.
ift:state_OFF
  rdf:type ift:machinestate ;
  rdfs:label "OFF" ;
  ift:stateValidFor ift:cutter ;
  ift:stateValidFor ift:filter ;
.
ift:state_ON
  rdf:type ift:machinestate ;
  rdfs:label "ON" ;
  ift:stateValidFor ift:cutter ;
  ift:stateValidFor ift:filter ;
.
ift:state_PREPARING
  rdf:type ift:machinestate ;
  rdfs:label "PREPARING" ;
  ift:stateValidFor ift:cutter ;
.
ift:state_PROCESSING
  rdf:type ift:machinestate ;
  rdfs:label "PROCESSING" ;
  ift:stateValidFor ift:cutter ;
.
ift:wasteclass
  rdf:type rdfs:Class ;
.
ift:workpiece
  rdf:type rdfs:Class ;
  rdfs:subClassOf ift:entity ;
.

